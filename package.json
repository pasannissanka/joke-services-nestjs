{
  "name": "joke-services",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:all": "yarn run build submit-jokes-service && yarn run build deliver-jokes-service && yarn run build moderate-jokes-service",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev:submit": "nest start submit-jokes-service --watch",
    "start:dev:deliver": "nest start deliver-jokes-service --watch",
    "start:dev:moderate": "nest start moderate-jokes-service --watch",
    "start:dev:all": "concurrently -c \"green,cyan,blue\" \"yarn run start:dev:submit\" \"yarn run start:dev:deliver\" \"yarn run start:dev:moderate\"",
    "start:debug": "nest start --debug --watch",
    "start:prod:submit": "node dist/apps/submit-jokes-service/main",
    "start:prod:deliver": "node dist/apps/deliver-jokes-service/main",
    "start:prod:moderate": "node dist/apps/moderate-jokes-service/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/joke-services/test/jest-e2e.json"
  },
  "dependencies": {
    "@mikro-orm/cli": "^6.3.4",
    "@mikro-orm/core": "^6.3.4",
    "@mikro-orm/mongodb": "^6.3.4",
    "@mikro-orm/mysql": "^6.3.4",
    "@mikro-orm/nestjs": "^6.0.2",
    "@mikro-orm/seeder": "^6.3.4",
    "@nestjs/common": "10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "10.3.10",
    "@nestjs/microservices": "^10.3.10",
    "@nestjs/platform-express": "10.3.10",
    "@nestjs/swagger": "^7.4.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.4.3",
    "@nestjs/schematics": "10.1.3",
    "@nestjs/testing": "10.3.10",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "^20.3.1",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "^29.5.0",
    "prettier": "3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ]
  },
  "mikro-orm": {
    "configPaths": [
      "./apps/deliver-jokes-service/src/db/mikro-orm.config.ts"
    ]
  }
}
